  "typography": {
    "H1": {
      "fontFamily": "{fontFamilies.primary}",
      "fontWeight": "{fontWeights.heavy}",
      "lineHeight": "{lineHeights.tight}",
      "fontSize": "{fontSizes.lg}",
      "$type": "typography"
    },
    "H2": {
